{
  "expo": {
    "name": "meong-nyang-diary",
    "slug": "meong-nyang-diary",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "meong-nyang-diary",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.meongnyang.diary",
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.meongnyang.diary"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@react-native-kakao/core",
        {
          "nativeAppKey": "8ced6882a26c159f35da0f3db7e99309",
          "android": {
            "authCodeHandlerActivity": true
          },
          "ios": {
            "handleKakaoOpenUrl": true
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "extraMavenRepos": ["https://devrepo.kakao.com/nexus/content/groups/public/"]
          }
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.628998903521-uta5j62hjkbqe2auc6j1rq0sot45a76g"
        }
      ],
      "expo-apple-authentication",
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      },
      "kakaoNativeAppKey": "8ced6882a26c159f35da0f3db7e99309",
      "kakaoRestApiKey": "d5916d12cd5c4160cf7b015cebf0e253",
      "googleIosClientId": "628998903521-uta5j62hjkbqe2auc6j1rq0sot45a76g.apps.googleusercontent.com",
      "googleAndroidClientId": "YOUR_ANDROID_CLIENT_ID_HERE",
      "googleWebClientId": "628998903521-9jhj9819r60hr85gnd0nslgj32o61tr4.apps.googleusercontent.com"
    }
  }
}
